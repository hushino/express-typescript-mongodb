<% include ./layouts/header %>
<h1>contribuyente</h1>
<% serie.forEach(function(serie) { %>
					
    <% const serieUrl = '/contribuyente/' + serie.patente;   %>
<div class="sada1">
                <a href="contribuyente/<%- serie.patente %>" class="card-link">  <img style="max-width: 13% !important;" class="img-fluid" id="image-home" src="http://localhost:3000/<%- serie.image %>" alt="Card image cap"></a>
                    <h4 class="card-title">
                        <%- serie.patente.substring(0,18) %>
                    </h4>
                    <p class="card-text">
                        <%- serie.cuit.substring(0,20) %>...
                    </p>
            </div>
<% }); %>
</div>

<div class="pagination">
<% if (pages > 0) { %>
<ul class="pagination text-center">
<% if (current == 1) { %>
    <a class="disabled"><<</a>
<% } else { %>
    <a href="/contribuyente/1"><<</a>
<% } %>
<% var i = (Number(current) > 5 ? Number(current) - 4 : 1) %>
<% if (i !== 1) { %>
    <a class="disabled">...</a>
<% } %>
<% for (; i <= (Number(current) + 4) && i <= pages; i++) { %>
    <% if (i == current) { %>
        <a class="active"><%= i %></a>
    <% } else { %>
        <a href="/contribuyente/<%= i %>"><%= i %></a>
    <% } %>
    <% if (i == Number(current) + 4 && i < pages) { %>
        <a class="disabled">...</a>
    <% } %>
<% } %>
<% if (current == pages) { %>
    <a class="disabled">>></a>
<% } else { %>
   <a href="/contribuyente/<%= pages %>">>></a>
<% } %>
</ul>
<% } %>
</div>


<% include ./layouts/footer %>